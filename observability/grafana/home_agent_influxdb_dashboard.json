        {
          "datasource": {
            "type": "influxdb",
            "uid": "${DS_INFLUXDB}"
          },
          "query": "import \"array\"\n\nsuccesses = from(bucket: \"${bucket}\")\n  |> range(start: v.timeRangeStart, stop: v.timeRangeStop)\n  |> filter(fn: (r) => r[\"entity_id\"] == \"home_agent_tool_successes\")\n  |> filter(fn: (r) => r[\"domain\"] == \"counter\")\n  |> filter(fn: (r) => r[\"_field\"] == \"value\")\n  |> last()\n  |> findRecord(fn: (key) => true, idx: 0)\n\nfailures = from(bucket: \"${bucket}\")\n  |> range(start: v.timeRangeStart, stop: v.timeRangeStop)\n  |> filter(fn: (r) => r[\"entity_id\"] == \"home_agent_tool_failures\")\n  |> filter(fn: (r) => r[\"domain\"] == \"counter\")\n  |> filter(fn: (r) => r[\"_field\"] == \"value\")\n  |> last()\n  |> findRecord(fn: (key) => true, idx: 0)\n\nsuccessCount = float(v: successes._value)\nfailureCount = float(v: failures._value)\ntotal = successCount + failureCount\n\nrate = if total > 0.0 then (successCount / total) * 100.0 else 0.0\n\narray.from(rows: [{_time: now(), _value: rate}])",
          "refId": "A"
        }
      ],
      "title": "Tool Success Rate",
      "type": "gauge"
